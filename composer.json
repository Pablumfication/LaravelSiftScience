{
    "name": "Pablumfication/laravel-sift-science",
    "type": "library",
    "description": "Laravel Wrapper for the Sift Science API",
    "keywords": [
        "fraud",
        "sift science",
        "api"
    ],
    "homepage": "https://github.com/Pablumfication/laravel-sift-science",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Davidson",
            "email": "mark@4each.co.uk",
            "homepage": "https://pablumfication.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.6|~7.0",
        "siftscience/sift-php" : "2.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Pablumfication\\LaravelSiftScience\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pablumfication\\LaravelSiftScience\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
